<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<!-- 日志输出位置 -->
    <Appenders>
    	<!-- 在控制台输出  -->
        <Console name="STDOUT" target="SYSTEM_OUT">
            <!-- 输出日志格式 -->
            <PatternLayout pattern="%d %-5p %C{2} (%F:%L) - %m%n"/>
        </Console>
        
        
    </Appenders>
    <Loggers>
        <Root level="warn">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>